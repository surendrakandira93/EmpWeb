<div class="__position__header __title__header "><div>Positions</div><div style="text-transform: none;"></div></div>


<div class="__position__box__container">

    <div>
        <div class="squar__off__type">
            <div>

                <label class="checkbox-container">
                    <div class="checkbox-label">
                        @if (Model.SquareOff == "leg")
                        {
                            <input type="radio" name="squareOff" value="leg" checked="checked">
                        }
                        else
                        {
                            <input type="radio" name="squareOff" value="leg">
                        }

                        <span class="checkbox-custom radio__type"></span>
                    </div>
                    <div class="checkbox__title">Square Off One Leg</div>
                </label>

                <label class="checkbox-container">
                    <div class="checkbox-label">
                        @if (Model.SquareOff == "all")
                        {
                            <input type="radio" name="squareOff" value="all" checked="checked" readonly>
                        }
                        else
                        {
                            <input type="radio" name="squareOff" value="all" readonly>
                        }
                        <span class="checkbox-custom radio__type"></span>
                    </div>
                    <div class="checkbox__title">Square Off All Legs</div>
                </label>
            </div>
        </div>

        <div class="squar__off__type entry_condition">
            <div>
                <label class="checkbox-container">
                    <div class="checkbox-label">
                        @if (Model.IsWaitAndTrade)
                        {
                            <input type="checkbox" name="isWaitAndTrade" value="true" checked="checked" readonly>
                        }
                        else
                        {
                            <input type="checkbox" name="isWaitAndTrade" value="false" readonly>
                        }

                        <span class="checkbox-custom"></span>
                    </div>
                    <div class="checkbox__title">Wait &amp; Trade</div>
                </label>

                <label class="checkbox-container">
                    <div class="checkbox-label">
                        @if (Model.IsWaitAndTrade)
                        {
                            <input type="checkbox" name="isCTC" checked="checked" value="true" readonly>
                        }
                        else
                        {
                            <input type="checkbox" name="isCTC" value="false">
                        }


                        <span class="checkbox-custom"></span>
                    </div>
                    <div class="checkbox__title">Move SL to Cost</div>
                </label>
                <label class="checkbox-container">
                    <div class="checkbox-label">
                        @if (Model.IsWaitAndTrade)
                        {
                            <input type="checkbox" name="isReEntry" checked="checked" value="true" readonly>
                        }
                        else
                        {
                            <input type="checkbox" name="isReEntry" value="false">
                        }
                        <span class="checkbox-custom"></span>
                    </div>
                    <div class="checkbox__title">Re-Entry</div>
                </label>
            </div>
        </div>
    </div>

    <div>
        @foreach (var row in Model.SelectedPosition)
        {
            <div class="__position__box row" id="po_row_@row.Id">
                <div class="col">
                    <label class="checkbox-container">
                        <div class="checkbox-label">
                            <input type="checkbox" value="true" name="model[@row.Id].IsChecked" class="poss_check" data-index="0" checked="checked" readonly>
                            <span class="checkbox-custom"></span>
                        </div>
                    </label>
                </div>

                <div class="col">
                    <div class="__position__details">
                        <div class="__left__align__leginfo">
                            <div class="total_lot" data-val="sp_total_lot" data-index="0" data-name="totalLot" style="text-transform: lowercase; padding: 0px;">
                                <input type="hidden" name="model[@row.Id].TotalLot" value="@row.TotalLot" />
                                <span class="sp_total_lot">@row.TotalLot</span>
                                <span style="padding: 2.5px 0px 0px 3px; transform: rotate(45deg); font-size: 14px; text-transform: capitalize;">Lots</span>
                            </div>
                            <div title="Click to change" class="position__button">
                                <input type="hidden" name="model[@row.Id].ActionType" value="@row.ActionType" />
                                @row.ActionType
                            </div>

                            <div class="strike_price">
                                <input type="hidden" name="model[@row.Id].StrikePrice" value="@row.StrikePrice" />
                                <input type="hidden" name="model[@row.Id].ClosestPremium" value="@row.ClosestPremium" />
                                @if (row.Segment == "options")
                                {
                                    <span class="sp_strikePrice">
                                        ~@row.ClosestPremium pt
                                    </span>
                                }
                                else if (row.EntryType == "cp")
                                {
                                    <span class="sp_strikePrice">
                                        @(row.StrikePrice == 0 ? "ATM" : (row.StrikePrice > 0 ? "ATM+" + row.StrikePrice : "ATM" + row.StrikePrice)) ATM+900
                                    </span>

                                }
                                else
                                {
                                    <div class="sp_strikePrice"> @row.Segment</div>
                                }


                            </div>
                            <div title="Click to change" class="position__button">
                                <input type="hidden" name="model[@row.Id].OptionType" value="@row.OptionType" />
                                @row.OptionType
                            </div>
                        </div>


                        <div class="__right__align__leginfo">

                            <input type="hidden" name="model[@row.Id].EntryWait.Type" value="@row.EntryWait.Type" />
                            <input type="hidden" name="model[@row.Id].EntryWait.Value" value="@row.EntryWait.Value" />

                            @if (Model.IsWaitAndTrade)
                            {
                                <div class="__box__input select__box__input large__select">



                                    <select class="targetProfitselect" data-name="entryWait" data-index="0">
                                        @if (row.EntryWait.Type == "wp_+")
                                        {
                                            <option value="wp_+" selected>🧭  % ↑</option>
                                            <option value="wp_-">🧭  % ↓</option>
                                            <option value="wpn_+">🧭  pt ↑</option>
                                            <option value="wpn_-">🧭  pt ↓</option>
                                        }
                                        else if (row.EntryWait.Type == "wp_-")
                                        {
                                            <option value="wp_+">🧭  % ↑</option>
                                            <option value="wp_-" selected>🧭  % ↓</option>
                                            <option value="wpn_+">🧭  pt ↑</option>
                                            <option value="wpn_-">🧭  pt ↓</option>
                                        }
                                        else if (row.EntryWait.Type == "wpn_+")
                                        {
                                            <option value="wp_+">🧭  % ↑</option>
                                            <option value="wp_-">🧭  % ↓</option>
                                            <option value="wpn_+" selected>🧭  pt ↑</option>
                                            <option value="wpn_-">🧭  pt ↓</option>
                                        }
                                        else
                                        {
                                            <option value="wp_+">🧭  % ↑</option>
                                            <option value="wp_-">🧭  % ↓</option>
                                            <option value="wpn_+">🧭  pt ↑</option>
                                            <option value="wpn_-" selected>🧭  pt ↓</option>
                                        }

                                    </select>

                                    <input type="number" placeholder="0" class="__box__input targetProfitinput" value="@row.EntryWait.Value">
                                </div>
                            }


                            <input type="hidden" name="model[@row.Id].TargetProfit.Status" value="@row.TargetProfit.Status" />

                            @if (row.TargetProfit.Status)
                            {
                                <div class="add__leg__tpsl">
                                    <div class="__box__input select__box__input">
                                        <select name="model[@row.Id].TargetProfit.Type">
                                            @if (row.TargetProfit.Type == "tpp")
                                            {
                                                <option value="tpp" selected="selected">TP %</option>
                                                <option value="tppn">TP pt</option>
                                            }
                                            else
                                            {
                                                <option value="tpp">TP %</option>
                                                <option value="tppn" selected="selected">TP pt</option>

                                            }

                                        </select>
                                        <input class="__box__input" name="model[@row.Id].TargetProfit.Value" placeholder="0" value="@row.TargetProfit.Value">
                                    </div>
                                </div>
                            }
                            else
                            {
                                <input type="hidden" name="model[@row.Id].TargetProfit.Type" value="@row.TargetProfit.Type" />
                                <input type="hidden" name="model[@row.Id].TargetProfit.Value" value="@row.TargetProfit.Value" />
                            }


                            <input type="hidden" name="model[@row.Id].StopLoss.Status" value="@row.StopLoss.Status" />
                            @if (row.StopLoss.Status)
                            {
                                <div class="add__leg__tpsl">
                                    <div class="__box__input select__box__input large__select">
                                        <select name="model[@row.Id].StopLoss.Type">
                                            @if (row.TargetProfit.Type == "tpp")
                                            {
                                                <option value="slp" selected="selected">SL %</option>
                                                <option value="slpn">SL pt</option>
                                            }
                                            else
                                            {
                                                <option value="slp">SL %</option>
                                                <option value="slpn" selected="selected">SL pt</option>

                                            }

                                        </select>
                                        <input class="__box__input" type="number" name="model[@row.Id].StopLoss.Value" placeholder="0" value="@row.StopLoss.Value">
                                    </div>

                                </div>
                            }
                            else
                            {
                                <input type="hidden" name="model[@row.Id].StopLoss.Type" value="@row.StopLoss.Type" />
                                <input type="hidden" name="model[@row.Id].StopLoss.Value" value="@row.StopLoss.Value" />
                            }

                            <input type="hidden" name="model[@row.Id].TrailingStopLoss.Status" value="@row.TrailingStopLoss.Status" />
                            <input type="hidden" name="model[@row.Id].TrailingStopLoss.Type" value="@row.TrailingStopLoss.Type" />
                            <input type="hidden" name="model[@row.Id].TrailingStopLoss.XValue" value="@row.TrailingStopLoss.XValue" />
                            <input type="hidden" name="model[@row.Id].TrailingStopLoss.YValue" value="@row.TrailingStopLoss.YValue" />


                            @if (Model.IsReEntry)
                            {
                                <div class="add__leg__tpsl trailing__box">
                                    <div class="__box__input label_group">
                                        <div class="label">
                                            Re-entry
                                        </div>
                                        <select name="model[@row.Id].TrailingStopLoss.Type">
                                            @if (row.ReEntry.Value == 1)
                                            {
                                                <option value="1" selected>1</option>
                                                <option value="2">2</option>
                                                <option value="3">3</option>
                                                <option value="4">4</option>
                                                <option value="5">5</option>
                                                <option value="6">6</option>
                                            }
                                            else if (row.ReEntry.Value == 2)
                                            {
                                                <option value="1">1</option>
                                                <option value="2" selected>2</option>
                                                <option value="3">3</option>
                                                <option value="4">4</option>
                                                <option value="5">5</option>
                                                <option value="6">6</option>
                                            }
                                            else if (row.ReEntry.Value == 3)
                                            {
                                                <option value="1">1</option>
                                                <option value="2">2</option>
                                                <option value="3" selected>3</option>
                                                <option value="4">4</option>
                                                <option value="5">5</option>
                                                <option value="6">6</option>
                                            }
                                            else if (row.ReEntry.Value == 4)
                                            {
                                                <option value="1">1</option>
                                                <option value="2">2</option>
                                                <option value="3">3</option>
                                                <option value="4" selected>4</option>
                                                <option value="5">5</option>
                                                <option value="6">6</option>
                                            }
                                            else if (row.ReEntry.Value == 5)
                                            {
                                                <option value="1">1</option>
                                                <option value="2">2</option>
                                                <option value="3">3</option>
                                                <option value="4">4</option>
                                                <option value="5" selected>5</option>
                                                <option value="6">6</option>
                                            }
                                            else
                                            {
                                                <option value="1">1</option>
                                                <option value="2">2</option>
                                                <option value="3">3</option>
                                                <option value="4">4</option>
                                                <option value="5">5</option>
                                                <option value="6" selected>6</option>
                                            }

                                        </select>
                                    </div>

                                </div>
                            }
                            else
                            {
                                @if (row.TrailingStopLoss.Status)
                                {
                                    <div class="add__leg__tpsl trailing__box">
                                        <div class="__box__input select__box__doubleinput">
                                            <select name="model[@row.Id].TrailingStopLoss.Type">
                                                <option value="tslp" selected="selected">TSL %</option>
                                                <option value="tslpn">TSL pt</option>
                                            </select>
                                            <input class="__box__input" type="number" name="model[@row.Id].TrailingStopLoss.XValue" placeholder="X" value="@row.TrailingStopLoss.XValue">
                                            <input class="__box__input" type="number" name="model[@row.Id].TrailingStopLoss.YValue" placeholder="Y" value="@row.TrailingStopLoss.YValue">
                                        </div>

                                    </div>
                                }
                            }


                            <div title="Click to change" class="position__button __expiry__type__button">

                                <input name="model[@row.Id].Stock" type="hidden" value="@row.Stock">
                                @row.Stock
                            </div>

                            <div title="Click to change" class="position__button __expiry__type__button">

                                <input name="model[@row.Id].ExpiryType" type="hidden" value="@row.ExpiryType">
                                @row.ExpiryType
                            </div>
                        </div>

                    </div>
                </div>

                <div class="col">

                </div>
            </div>
        }





    </div>
</div>

